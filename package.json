{
  "name": "e-com",
  "version": "0.0.0",
  "scripts": {
    "build": "nx run-many --target=build --parallel=5",
    "build:affected": "nx affected:build --base=HEAD --parallel=5",
    "build:packages": "nx run-many --target=build --exclude @e-com/host --parallel=5",
    "test": "nx run-many --target=test --parallel=5",
    "test:packages": "nx run-many --target=test --exclude @e-com/host --parallel=5",
    "lint": "nx run-many --target=lint --parallel=5",
    "lint:fix": "nx run-many --target=lint:fix",
    "dev": "nx run @e-com/host:serve",
    "preview": "nx run-many --target=preview",
    "codegen": "nx run-many --target=codegen",
    "graph": "nx graph",
    "deph-graph": "nx affected:dep-graph --base=HEAD"
  },
  "private": true,
  "devDependencies": {
    "@nx/eslint": "18.0.4",
    "@nx/eslint-plugin": "18.0.4",
    "@nx/jest": "18.0.4",
    "@nx/js": "18.0.4",
    "@nx/plugin": "^18.0.4",
    "@nx/vite": "^18.0.4",
    "@nx/web": "18.0.4",
    "@nx/workspace": "^18.0.4",
    "@swc-node/register": "~1.8.0",
    "@swc/cli": "~0.1.62",
    "@swc/core": "^1.4.0",
    "@swc/helpers": "~0.5.2",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitest/ui": "^1.0.4",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "nx": "18.0.4",
    "prettier": "^2.6.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "^5.3.3",
    "vite": "~5.0.0",
    "vitest": "^1.0.4",
    "@nrwl/nx-plugin": "^18.0.4",
    "@nx/devkit": "18.0.4",
    "tslib": "^2.3.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.10.0",
    "@nx/nx-linux-x64-gnu": "18.0.1",
    "@nx/nx-win32-x64-msvc": "18.0.1"
  }
}
